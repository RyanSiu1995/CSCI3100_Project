<p id="notice"><%= notice %></p>

<%= link_to 'Edit', edit_trip_path(@trip) %> |
<%= link_to 'Back', trips_path %>

<% provide(:title, @trip.title) %>
<div class="row">
  <aside class="col-md-4">
    <section class="trip_info">
      <h1>
        <%= @trip.title %>
      </h1>
      <h2>
        <%= @trip.country %>
      </h2>
      From: <%= @trip.start_date %>
      To: <%= @trip.finish_date %>

    </section>

</div>
